{
	"uid": "github.com/fireconsole/widget.console/loops",
	"name": "loops",
	"version": "0.0.0",
	"main": "./loader.js",
	"mappings": {
		"q": "./node_modules/q",
		"insight.renderers.default": "./node_modules/insight.renderers.default",
        "modules": {
        	"depends": [
        		"fp-modules-for-nodejs"
        	],
        	"location": "./node_modules/fp-modules-for-nodejs"
        },
        "insight": {
        	"depends": [
        		"insight-for-js"
        	],
        	"location": "./node_modules/insight-for-js"
        },
		"domplate": "./node_modules/domplate",
		"insight.renderers.default":  {
        	"depends": [
        		"domplate"
        	],
        	"location": "https://github.com/insight/insight.renderers.default/archive/v0.0.1.tar.gz"
        }
	},
	"dependencies": {
		"fp-modules-for-nodejs": "0.2.x",
		"insight-for-js": "0.1.x",
		"domplate": "0.2.x",
		"q": "~1.1.2"
	},
    "scripts": {
        "install": "../node_modules/.bin/smi install"
    },
    "config": {
	    "smi.cli": {
	        "packagesDirectory": "node_modules"
	    }
    }
}